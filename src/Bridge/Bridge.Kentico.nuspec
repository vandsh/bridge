                <dependency id="Nancy" version="2.0.0" />
                <dependency id="Nancy.Authentication.Basic" version="2.0.0" />
                <dependency id="Nancy.Hosting.Aspnet" version="2.0.0" />
                <dependency id="YamlDotNet" version="8.0.0" />
                <dependency id="Mapster" version="4.1.1" />
                <dependency id="DiffPlex" version="1.5.0" />
            </group>
        </dependencies>
    </metadata>
    <files>
        <file src="static\logo.png" target="static\" />
        <file src="bin\Bridge.dll" target="lib\net461\" />
        <file src="web.config.install.xdt" target="content\web.config.install.xdt" />
        <file src="static\**" target="content\Bridge\static" />
        <file src="Views\**" target="content\Bridge\Views" />
