<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <id>Bridge.Kentico</id>
        <version>0.1.0-alpha</version>
        <authors>Heartland Business Systems</authors>
        <owners>vandsh</owners>
        <license type="expression">MIT</license>
        <projectUrl>https://github.com/vandsh/bridge</projectUrl>
        <icon>static\logo.png</icon>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>A YAML based tool for facilitating the promotion of Kentico database items</description>
